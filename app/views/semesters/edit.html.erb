<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_for(@semester) do |f| %>
        <%= render(:partial => 'form', :locals =>{:f => f})%>
        <div class="action">
          <%= f.submit "Update", class:"btn btn-primary" %>
          <%= link_to "Cancel", semesters_path, class:"btn btn-danger", style:"color:white" %>
        </div>
    <% end %>
  </div>
</div>

